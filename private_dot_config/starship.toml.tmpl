[battery]
full_symbol = "󲀙 "
charging_symbol = "󲀖 "
discharging_symbol = "󲀉 "
unknown_symbol = "󲀀 "
empty_symbol = "󲀀 "
format='[$symbol]($style)'

[[battery.display]]
threshold = 10
charging_symbol = "󲀎 "
discharging_symbol = "󲀁 "
style = 'fg:sys-red-da bg:base'

[[battery.display]]
threshold = 20
charging_symbol = "󲀏 "
discharging_symbol = "󲀂 "
style = 'fg:sys-red-da bg:base'

[[battery.display]]
threshold = 25
charging_symbol = "󲀏 "
discharging_symbol = "󲀃 "
style = 'fg:sys-red-da bg:base'

[[battery.display]]
