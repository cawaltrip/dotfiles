[directory]
style = "bg:red fg:base"
format = "[ $path ]($style)"
truncation_length = 3
truncation_symbol = "…/"
truncate_to_repo = true

[directory.substitutions]
"workplace" = " "
"Documents" = " "
"Downloads" = " "
"Music" = " "
"Pictures" = " "
"tmp" = "󱤤 "

[aws]
symbol = "󰸏 "
style = "fg:#ff9900"

[git_branch]
symbol = ""
style = "bg:yellow fg:base"
format = '[ $symbol $branch ]($style)'

[git_status]
style = "bg:yellow fg:base"
format = '[$all_status$ahead_behind ]($style)'

[time]
disabled = false
time_format = "%R" # Hour:Minute Format
use_12hr = false
style = "bg:blue fg:base"
format = '[ $time ]($style)'

[status]
style = 'fg:sys-red-da bg:base'
symbol = '✖'
format = '[$symbol$signal_name $int ]($style)'
map_symbol = true
disabled = false

[python]
style = "bg:python-blue fg:base"
version_format = 'v${major}.${minor}'
symbol = ' '
pyenv_version_name = true
# pyenv_prefix = 
format = '[${symbol}(${version} )(\($virtualenv\) )]($style)'

[nodejs]
style = "bg:nodejs-green fg:text"
symbol = ' '
version_format = 'v${major}.${minor}'
format = '[$symbol($version )]($style)'

